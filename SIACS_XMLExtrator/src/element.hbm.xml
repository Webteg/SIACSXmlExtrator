<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="model.business.Element" table="element" >
		<meta attribute="class-description"> This class contains element details. </meta>
        
		<id name="id_element" type="integer" column="id_element">
			<generator class="native" />
		</id>
		        
        <property name="root_name" type="string" column="root_name" length="120" />
        <property name="children_name" type="string" column="children_name" length="120" />
      
    </class>

</hibernate-mapping>
